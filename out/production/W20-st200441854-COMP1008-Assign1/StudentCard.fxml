<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchor" prefHeight="410.0" prefWidth="541.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StudentCardController">
   <children>
      <HBox layoutX="12.0" layoutY="14.0" prefHeight="380.0" prefWidth="515.0" spacing="10.0">
         <children>
            <ImageView id="imageView" fx:id="imageView" fitHeight="196.0" fitWidth="190.0" pickOnBounds="true" preserveRatio="true" styleClass="Image_View">
               <effect>
                  <DropShadow height="42.45" radius="26.0875" width="63.9">
                     <color>
                        <Color opacity="0.6885812878608704" />
                     </color>
                  </DropShadow>
               </effect>
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </HBox.margin>
               <viewport>
                  <Rectangle2D />
               </viewport></ImageView>
            <VBox prefHeight="371.0" prefWidth="347.0" spacing="45.0">
               <children>
                  <Label fx:id="firstName" alignment="CENTER" text="First Name:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="lastName" text="Last Name:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="studentNumber" text="Student#">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="listActivities" maxWidth="300.0" minWidth="300.0" prefHeight="90.0" prefWidth="300.0" />
                  <Button mnemonicParsing="false" onAction="#listOfActivitiesPage" text="Go to list of Activities" />
               </children>
            </VBox>
         </children>
      </HBox>
      

   </children>
</AnchorPane>
